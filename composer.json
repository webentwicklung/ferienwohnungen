{
	"name":"webentwicklung/ferienwohnungen",
	"description":"Freienwohnung Erweiterung fuer Contao.",
	"keywords":["contao","ferienwohnung","extension", "erweiterung"],
	"type":"contao-bundle",
	"homepage":"http://www.webentwicklung.tk",
	"license":"MIT",
	"authors":[
		{
			"name":"Tino Kupke",
			"homepage":"http://www.webentwicklung.tk",
			"role":"Developer"
		}
	],
	"support":{
		"source":"https://github.com/mopic810/contao-standard-extension"
	},
	"require":{
		"php":">=7.1",
		"contao/core-bundle":"^4.3"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0"
	},
	"conflict": {
		"contao/core": "*",
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"autoload":{
		"psr-4": {
			"Wtk\\Fewo\\": "src/"
		}
	},
	"extra": {
		 "contao-manager-plugin": "Wtk\\Fewo\\ContaoManagerPlugin"
	},
	"replace":{
		"contao-legacy/fewo-extension":"self.version"
	}
}
